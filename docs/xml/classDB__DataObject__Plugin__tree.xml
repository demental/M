        <definition>DB_DataObject_Plugin_tree::_generateTreeData</definition>
        <argsstring>(&amp;$obj, $parent, &amp;$arr, $level, &amp;$n)</argsstring>
        <name>_generateTreeData</name>
        <param>
          <type>&amp;$</type>
